{% extends 'admin.html.twig' %}

{% block page_title %} Mostrando {% endblock %}
{% block page_subtitle %} Oferta acad√©mica {% endblock %}

{% block page_content %}

    <table id="tabla_seccion" class="table table-bordered table-striped">
        <tbody>
            <tr>
                <th>Nombre</th>
                <td>{{ seccion.nombre }}</td>
            </tr>
            <tr>
                <th>Aula</th>
                <td>{{ seccion.aula }}</td>
            </tr>
            <tr>
                <th>Cupo</th>
                <td>{{ seccion.cupo }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ seccion.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_oferta_seccion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_oferta_seccion_edit', { 'id': seccion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
