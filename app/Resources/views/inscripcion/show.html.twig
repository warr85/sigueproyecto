{% extends 'base.html.twig' %}

{% block body %}
    <h1>Inscripcion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inscripcion.id }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if inscripcion.created %}{{ inscripcion.created|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Modified</th>
                <td>{% if inscripcion.modified %}{{ inscripcion.modified|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_inscripcion_mostrar' , { 'id': inscripcion.idEstadoAcademico.id }) }}">Regresar a la Inscripcion</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="eliminar UC">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
